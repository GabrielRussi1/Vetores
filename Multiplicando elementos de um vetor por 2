// Multiplicando elementos de um vetor por 2
// Gabriel Russi

#include <stdio.h>

int main() {
    int vetor[5] = {10, 20, 30, 40, 50};
    int i;

    for (i = 0; i < 5; i++) {
        vetor[i] = vetor[i] * 2;
    }

    printf("Os elementos do vetor multiplicados por 2 sao:\n");
    for (i = 0; i < 5; i++) {
        printf("vetor[%d] = %d\n", i, vetor[i]);
    }

    return 0;
}
