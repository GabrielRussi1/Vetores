// Contando numeros pares em um vetor
// Gabriel Russi

#include <stdio.h>

int main() {
    int vetor[5] = {10, 21, 30, 41, 50};
    int i, contagem = 0;

    for (i = 0; i < 5; i++) {
        if (vetor[i] % 2 == 0) {
            contagem++;
        }
    }

    printf("O numero de elementos pares no vetor e: %d\n", contagem);

    return 0;
}
