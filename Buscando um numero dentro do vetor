// Buscando um numero dentro do vetor
// Gabriel Russi

#include <stdio.h>

int main() {
    int vetor[5] = {10, 20, 30, 40, 50};
    int num, i, encontrado = 0;

    printf("Digite um numero para buscar no vetor: ");
    scanf("%d", &num);

    for (i = 0; i < 5; i++) {
        if (vetor[i] == num) {
            encontrado = 1;
            break;
        }
    }

    if (encontrado) {
        printf("O numero %d foi encontrado no vetor.\n", num);
    } else {
        printf("O numero %d nao foi encontrado no vetor.\n", num);
    }

    return 0;
}
