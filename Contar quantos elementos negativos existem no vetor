// Contar quantos elementos negativos existem no vetor
// Gabriel Russi

#include <stdio.h>

int main() {
    int vetor[5] = {-10, 20, -30, 40, -50};
    int i, contagem = 0;

    for (i = 0; i < 5; i++) {
        if (vetor[i] < 0) {
            contagem++;
        }
    }

    printf("O numero de elementos negativos no vetor e: %d\n", contagem);

    return 0;
}
