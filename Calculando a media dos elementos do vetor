// Calculando a media dos elementos do vetor
// Gabriel Russi

#include <stdio.h>

int main() {
    int vetor[5] = {10, 20, 30, 40, 50};
    int i, soma = 0;
    float media;

    for (i = 0; i < 5; i++) {
        soma += vetor[i];
    }

    media = soma / 5.0;

    printf("A media dos elementos do vetor e: %.2f\n", media);

    return 0;
}
