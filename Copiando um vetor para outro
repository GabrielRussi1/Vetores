// Copiando um vetor para outro
// Gabriel Russi

#include <stdio.h>

int main() {
    int vetor1[5] = {10, 20, 30, 40, 50};
    int vetor2[5];
    int i;

    for (i = 0; i < 5; i++) {
        vetor2[i] = vetor1[i];
    }

    printf("Os elementos do vetor2 sao:\n");
    for (i = 0; i < 5; i++) {
        printf("vetor2[%d] = %d\n", i, vetor2[i]);
    }

    return 0;
}
